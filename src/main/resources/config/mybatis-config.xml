<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC 
		"-//mybatis.org//DTD Config 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.pro.dao.Member" alias="member" />
		<typeAlias type="com.pro.dao.Board" alias="board" />
		<typeAlias type="com.pro.dao.blogBoard" alias="blog" />
		<typeAlias type="com.pro.dao.Reply" alias="reply" />
		<typeAlias type="com.pro.dao.ReReply" alias="rereply" />
		<typeAlias type="com.pro.dao.QNAVO" alias="qna"/>
		<typeAlias type="com.pro.dao.AddFriend" alias="addfriend"/>
	</typeAliases>
	
</configuration>
